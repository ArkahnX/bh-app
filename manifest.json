{
	"name": "Biotech-Hunters",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "Packaged app to view Biotech-Hunters Lore.",
	"icons": {
		"16": "images/16.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"app": {
		"background": {
			"scripts": ["js/background.js","js/jquery-1.7.2.min.js"]
		}
	},
	"incognito": "split",
	"minimum_chrome_version": "24",
	"offline_enabled": true,
	"permissions": ["unlimitedStorage","notifications","idle"]
}